{
    "$schema": "../../../tests.schema",
    "$kind": "Microsoft.Test.Script",
    "description": "Test HttpRequestMock",
    "httpRequestMocks": [
        "httpexact.mock",
        "httppartial.mock",
        "get.mock",
        "all.mock"
    ],
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "triggers": [
            {
                "$kind": "Microsoft.OnBeginDialog",
                "actions": [
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content.data}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.statusCode} ${dialog.result.reasonPhrase}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.statusCode} ${dialog.result.reasonPhrase}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "POST",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "POST",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content.data}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "POST",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content.data}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "method": "PATCH",
                        "url": "http://127.0.0.1",
                        "body": "full body or partial body",
						"contentType": "text/plain",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "PATCH",
                        "body": "full body",
						"contentType": "text/plain",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "Json",
                        "method": "POST",
                        "url": "http://127.0.0.2",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.statuscode} ${dialog.result.reasonphrase}"
                    }
                ]
            },
            {
                "$kind": "Microsoft.OnError",
                "actions": [
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "404 no matching mock handler for"
                    }
                ]
            }
        ],
        "autoEndDialog": false
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserSays",
            "text": "hello"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "string response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "object response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "byte array response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "assertions": [
                "contains(toLower(text),  '404 not found')"
            ]
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "assertions": [
                "contains(toLower(text),  '500 internal server error')"
            ]
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "fallback response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated ByteArray response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated ByteArray response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "partial match"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "exact match"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "assertions": [
                "contains(toLower(text), \"404 no matching mock handler for\")"
            ]
        }
    ]
}
